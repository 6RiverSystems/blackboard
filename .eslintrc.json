{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": ["6river/typescript"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"project": ["./tsconfig.json"]
	},
	"rules": {
		"new-cap": "off",
		"no-console": "off",
		"no-invalid-this": "off",
		"indent": ["error", "tab", {
		}]
	}
}
